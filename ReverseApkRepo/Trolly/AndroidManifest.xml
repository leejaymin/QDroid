<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="6" android:versionName="1.4" package="caldwell.ben.trolly"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="3" />
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <provider android:name=".TrollyProvider" android:authorities="caldwell.ben.provider.Trolly" />
        <activity android:label="@string/app_name" android:icon="@drawable/icon_48" android:name=".Trolly" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="org.openintents.action.INSERT_FROM_EXTRAS" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="org.openintents.type/string.arraylist.shopping" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".TrollyPreferences" />
    </application>
</manifest>
