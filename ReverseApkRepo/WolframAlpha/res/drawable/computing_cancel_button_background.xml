<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" android:drawable="@drawable/highlight_pressed" />
    <item android:state_focused="true" android:drawable="@drawable/highlight_selected" />
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0px" android:color="@color/gray_dimming" />
            <corners android:radius="3.0dip" />
            <solid android:color="@color/computing_button_background" />
        </shape>
    </item>
</selector>
