<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="300" android:versionName="3.0.0" package="com.mattcarver.clicker"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:label="@string/app_name" android:icon="@drawable/icon" android:debuggable="false">
        <activity android:theme="@android:style/Theme.Light.NoTitleBar" android:label="@string/app_name" android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="Settings" android:name="SettingsActivity" />
    </application>
    <uses-sdk android:minSdkVersion="3" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
</manifest>
