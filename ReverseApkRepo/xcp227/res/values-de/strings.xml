    <string name="set_camera_quality_summary">Qualität für die Bildkomprimierung (je höher desto besser).</string>
    <string name="set_camera_previewtime_title">Bildvorschau-Dauer</string>
    <string name="set_camera_previewtime_summary">Dauer in Sekunden, bis die Vorschau geschlossen wird. Ein Wert von 0 bedeutet, dass die Vorschau nicht angezeigt wird.</string>
    <string name="set_camera_gallerypreview_title">Zeige Vorschau in Gallery-App</string>
    <string name="set_camera_gallerypreview_summary">Wenn diese Option aktiviert ist, dann wird das Vorschau-Bild in der Standard-Gallery-App angezeigt (anstatt in CameraPro).</string>
    <string name="set_camera_capture_touch_focus_title">Nach Touch-Fokus aufzeichnen</string>
    <string name="set_camera_capture_touch_focus_summary">Wenn diese Option aktiviert ist, dann wird nach dem Touch-Fokusieren die Aktion des Software Auslöseknopf ausgeführt.</string>
    <string name="set_camera_capturebutton_mode_summary">Definiert die Aktion die ausgeführt wird, wenn der Software-Auslöseknopf gedrückt wird (nur bei Geräten, die Autofokus unterstützen)</string>
    <string name="set_camera_capturebutton_mode_title">Software Auslöseknopf</string>
    <string name="set_camera_exif_title">EXIF-Datum Fix</string>
    <string name="set_camera_exif_summary">Bei einigen Custom-Firmwares kann das EXIF Datum falsch sein. Diese Option überschreibt das Datum mit der Systemzeit.</string>
    <string name="set_video_header">Videoeinstellungen</string>
    <string name="set_video_note">Hinweis: der Video-Modus ist noch im Beta (Test) Status!</string>
    <string name="set_video_profile_title">Video Profil</string>
    <string name="set_video_profile_summary">Wählen Sie eine Videoqualitäts-Einstellung aus den vordefinierten Profilen.</string>
    <string name="set_video_duration_title">Maximale Aufnahmedauer</string>
    <string name="set_video_duration_summary">Dieser Wert gibt die maximale Länge einer Video-Datei in Sekunden an. Stellen Sie den Wert auf 0 für eine automatische Auswahl.</string>
    <string name="set_video_filesize_title">Maximale Dateigröße</string>
    <string name="set_video_filesize_summary">Dieser Wert gibt die maximale Dateigröße einer Video-Datei in MB an. Stellen Sie den Wert auf 0 für eine automatische Auswahl.</string>
    <string name="set_video_autosplit_title">Automatisches Datei-Splitting</string>
    <string name="set_video_autosplit_summary">Wenn ausgewählt, dann wird automatisch eine neue Video-Datei erstellt, wenn die maximale Aufnahmedauer oder die maximale Dateigröße erreicht wurde. Dies ist nützlich, um Aufnahmelimits zu umgehen.</string>
    <string name="set_video_autoplay_title">Video abspielen nach der Aufnahme</string>
    <string name="set_video_autoplay_summary">Das Video wird nach der Aufnahme in der Standard-Video-App wiedergegeben (nur wenn automatisches Splitting deaktiviert)</string>
    <string name="set_video_expert_title">Aktivieren Sie Video-Experten-Modus</string>
    <string name="set_video_expert_summary">Wenn diese Option aktiviert ist, dann wird das Video Profil von oben ignoriert. Hinweis: Änderungen in den Videoeinstellungen können ungültig sein und zu Abstürzen führen</string>
    <string name="set_video_copyprofile_title">Kopiere die Profileinstellungen</string>
    <string name="set_video_copyprofile_summary">Wenn diese Option aktiviert ist, dann werden die Standard-Profil-Einstellungen in die Expert-Einstellungen kopiert.</string>
    <string name="set_video_splitdelay_title">Video Split-Verzögerung</string>
    <string name="set_video_splitdelay_summary">Dieser Wert gibt die Verzögerung in ms zwischen zwei Videodateien an. Diese Verzögerung ist notwendig, da die Kamera reinitialisiert werden muss. Empfohlen: 1000ms oder höher.</string>
    <string name="set_video_record_audio_title">Audio aufnehmen</string>
    <string name="set_video_audiochannels_title">Anzahl Audio-Kanäle</string>
    <string name="set_video_audiochannels_summary">Stellen Sie die Anzahl auf 0 für automatische Auswahl.</string>
    <string name="set_video_audiocodec_title">Audio-Codec</string>
    <string name="set_video_audiobitrate_title">Audio-Encoding-Bitrate</string>
    <string name="set_video_audiobitrate_summary">Die Bitrate für Audio-Aufnahme in Bits / Sekunde. Setzen Sie diesen Wert auf 0 für eine automatische Auswahl.</string>
    <string name="set_video_audiosamplingrate_title">Audio-Sampling-Rate</string>
    <string name="set_video_audiosamplingrate_summary">Die Audio-Sampling-Rate in Samples / Sekunde. Setzen Sie diese Einstellung auf 0 für eine automatische Auswahl.</string>
    <string name="set_video_capturerate_title">Video-Aufzeichnungs-Rate</string>
    <string name="set_video_capturerate_summary">Setzen Sie den Wert auf 0 für eine automatische Auswahl. Dieser Wert kann verwendet werden um unterschiedlichen Aufnahme- und Wiedergaberaten zu definieren. Nur in Android 3+ und Zeitraffer Profilen verfügbar.</string>
    <string name="set_video_outputformat_title">Video-Ausgabeformat</string>
    <string name="set_video_outputformat_summary">Das Video-Container Ausgabeformat.</string>
    <string name="set_video_codec_title">Video-Codec</string>
    <string name="set_video_codec_summary">Das Video-Encoding-Format.</string>
    <string name="set_video_bitrate_title">Video-Encoding-Bitrate</string>
    <string name="set_video_bitrate_summary">Die Bitrate für Video-Aufzeichnung in Bits / Sekunde. Setzen Sie diesen Wert auf 0 für eine automatische Auswahl.</string>
    <string name="set_video_framerate_title">Video Bildrate</string>
    <string name="set_video_framerate_summary">Die Video-Bildrate in Bildern / Sekunde. Die tatsächliche Bildrate kann je nach Gerät und Lichtverhältnissen variieren. Setzen Sie diesen Wert auf 0 für eine automatische Auswahl.</string>
    <string name="set_video_width_title">Videobreite</string>
    <string name="set_video_width_summary">Die Breite des Videos in Pixel. Wählen Sie 0 für eine automatische Auswahl.</string>
    <string name="set_video_height_title">Videohöhe</string>
    <string name="set_video_height_summary">Die Höhe des Videos in Pixel. Wählen Sie 0 für eine automatische Auswahl.</string>
    <string name="set_gen_header">Allgemeine Einstellungen</string>
    <string name="set_gen_zoomslider_title">Zeige Zoom-Slider</string>
    <string name="set_gen_zoomslider_summary">Diese Funktion ist nur relevant für Geräte die Zoom unterstützen.</string>
    <string name="set_gen_volumekeys_title">Lautstärketasten zum Aufzeichnen</string>
    <string name="set_gen_volumekeys_summary">Wenn ausgewählt, werden die Lautstärketasten zum Aufzeichnen und Fokusieren verwendet, sonst zum Zoomen.</string>
    <string name="set_gen_resetapp_title">CameraPro zurücksetzen</string>
    <string name="set_gen_resetapp_summary">Setzt alle Kameraparameter beim Starten auf die Standardeinstellung zurück.</string>
    <string name="set_gen_mediabutton_title">Aufzeichnung mit Media-Taste</string>
    <string name="set_gen_mediabutton_summary">Wenn ausgewählt, dann können die Media-Tasten von Headsets für die Aufnahme verwendet werden.</string>
    <string name="set_gen_defaultapp_title">Standard-Kamera-App</string>
    <string name="set_gen_defaultapp_summary">Wenn ausgewählt, dann wird CameraPro gestartet, wenn der Hardware-Auslöser gedrückt wird (falls vorhanden).</string>
    <string name="set_gen_warnexit_title">Warnung beim Beenden</string>
    <string name="set_gen_warnexit_summary">Wenn ausgewählt, dann wird eine Warnung angezeigt, wenn CameraPro beendet wird.</string>
    <string name="set_gen_shuttersound_title">Auslösegeräusch</string>
    <string name="set_gen_shuttersound_summary">Wenn aktiviert, dann wird ein Auslösergeräusch abgespielt. Das Deaktivieren des Geräusches funktioniert nicht auf allen Geräten.</string>
    <string name="set_gen_touchfocus_title">Touch-Fokus</string>
    <string name="set_gen_touchfocus_summary">Android 4 +: wenn ausgewählt, dann wird der Fokus innerhalb des Fokusrechtecks berechnet, ansonsten automatisch über die gesamte Bildfläche.</string>
    <string name="set_gen_focusrect_title">Zeige Fokusrechteck</string>
    <string name="set_gen_focusrect_summary">Wenn ausgewählt, wird ein Rechteck zum Fokusieren angezeigt.</string>
    <string name="set_gen_focusbtn_title">Zeige Fokus-Knopf</string>
    <string name="set_gen_focusbtn_summary">Wenn ausgewählt, wird ein Software-Fokus-Knopf angezeigt.</string>
    <string name="set_gen_fullscreenviewfinder_title">Vollbild Sucher</string>
