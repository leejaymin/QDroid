<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="137" android:versionName="9.5" package="stericson.busybox.donate"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:theme="@style/CustomButton" android:label="BusyBox Pro" android:icon="@drawable/icon">
        <service android:name=".services.AppletService" />
        <activity android:label="BusyBox Pro" android:name=".Activity.MainActivity" android:launchMode="singleTop" android:configChanges="keyboard|keyboardHidden|orientation|screenLayout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".receivers.OnUpgradeReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REPLACED" />
                <data android:scheme="package" android:path="stericson.busybox.donate" />
            </intent-filter>
        </receiver>
    </application>
    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="11" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <supports-screens android:anyDensity="true" android:smallScreens="true" android:normalScreens="true" android:largeScreens="true" />
</manifest>
