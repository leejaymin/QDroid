<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="title_text">
        <item name="android:textSize">17.319977dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="title_button_text">
        <item name="android:textSize">13.319977dip</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="Transparent">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_default_transparent</item>
    </style>
    <style name="ThinkCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:button">@drawable/green_checkbox_selector</item>
        <item name="android:maxHeight">10.0dip</item>
    </style>
    <style name="Theme.NoBackground" parent="@android:style/Theme">
        <item name="android:windowBackground">@null</item>
    </style>
</resources>
