    <referenceDispatcher id="referenceDispatcher">
      <objRef refId="plusSelectionModeButton" refName="modeAddButton"/>
      <objRef refId="minusSelectionModeButton" refName="modeRemoveButton"/>
      <objRef refId="brush" refName="brushPanelButton"/>
      <objRef refId="edgeAwareCheckbox" refName="edgeAwareCheckBox"/>
    </referenceDispatcher>
  </objects>
  <classes>
    <class className="ttpanelbutton" superClassName="ttbasepanelbutton" packageName="com.adobe.ttpixel.ui" codeName="TTPanelButton">
