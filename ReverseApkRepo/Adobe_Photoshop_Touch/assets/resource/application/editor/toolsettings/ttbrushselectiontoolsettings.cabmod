        <ttplainbutton id="plusSelectionModeButton" width="50" height="50" x="0" y="0" target="referenceDispatcher" selector="modeAdd" toggles="true" type="tool" image="settingsModeAdd"/>
        <ttplainbutton id="minusSelectionModeButton" width="50" height="50" x="50" y="0" target="referenceDispatcher" selector="modeRemove" toggles="true" type="tool" image="settingsModeRemove"/>
        <ttpanelbutton id="brush" height="50" x="0" y="90" panelModuleID="brushPanelModule" target="referenceDispatcher" positionMode="6" isRound="true"/>
