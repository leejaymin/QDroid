        <text id="text11ttbold" height="44" x="0" y="212" text="$$$/ttbrushselectiontoolsettings/text/text/Edge Aware=Edge Aware" textSize="12" align="center"/>
        <ttslidecheckbox id="edgeAwareCheckbox" width="60" height="30" x="20" y="180" target="referenceDispatcher" selector="edgeAware" selected="true" isRound="true"/>
      </view>
    </window>
  </windows>
  <menus/>
