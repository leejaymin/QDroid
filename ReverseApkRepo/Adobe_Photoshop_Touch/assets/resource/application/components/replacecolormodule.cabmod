        <button id="button11" width="110" height="40" sizeX="right" x="630" y="60" target="referenceDispatcher" selector="CancelFilter" title="$$$/replacecolormodule/button/title/Cancel=Cancel"/>
        <imageView id="imageView" width="150" x="10" y="10"/>
        <ttliveslider id="slider2" width="240" height="40" sizeX="scale" x="370" y="110" target="referenceDispatcher" selector="threshold" minValue="-100" maxValue="100" endMouseSelector="thresholdLive" liveSelector="thresholdLive"/>
        <ttnumberfield id="textField11112" width="60" height="40" x="300" y="110" target="referenceDispatcher" text="$$$/replacecolormodule/textField/text/Text=Text" lostFocusSelector="thresholdLive" returnSelector="thresholdLive" parentPanel="module" unitString="%"/>
        <ttliveslider id="slider11" width="240" height="40" sizeX="scale" x="370" y="60" target="referenceDispatcher" selector="intensity" endMouseSelector="intensityLive" liveSelector="intensityLive"/>
        <ttnumberfield id="textField111111" width="60" height="40" x="300" y="60" target="referenceDispatcher" text="$$$/replacecolormodule/textField/text/Text=Text" lostFocusSelector="intensityLive" returnSelector="intensityLive" parentPanel="module" unitString="%"/>
