<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/3.1">
	<id>com.adobe.ttpixel.extension</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>library.jar</nativeLibrary>
				<initializer>com.adobe.ttpixel.extension.TTPixelExtension</initializer>
				<finalizer>com.adobe.ttpixel.extension.TTPixelExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libTTPixelExtensionCocoaTouch.a</nativeLibrary>
				<initializer>TTPixelExtensionInitializer</initializer>
				<finalizer>TTPixelExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<!-- 
		<platform name="default">
			<applicationDeployment />
		</platform>
		-->
	</platforms>
</extension>
