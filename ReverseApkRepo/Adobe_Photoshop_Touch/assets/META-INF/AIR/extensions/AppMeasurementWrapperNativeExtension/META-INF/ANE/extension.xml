<extension xmlns="http://ns.adobe.com/air/extension/2.5">
  <id>AppMeasurementWrapperNativeExtension</id>
  <versionNumber>1</versionNumber>
  <platforms>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>AppMeasurementWrapperAirAne-1.0.0-SNAPSHOT.jar</nativeLibrary>
        <initializer>com.adobe.omniture.AppMeasurementWrapperAirExtension</initializer>
        <finalizer>com.adobe.omniture.AppMeasurementWrapperAirExtension</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="iPhone-ARM">
      <applicationDeployment>
        <nativeLibrary>libAppMeasurementWrapperNativeExtension.a</nativeLibrary>
