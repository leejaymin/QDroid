        <Preference android:title="@string/prefs_title_back_color" android:key="@string/PREFSKEY_BACK_COLOR" android:summary="@string/prefs_summary_back_color" />
        <Preference android:title="@string/prefs_title_usage_color" android:key="@string/PREFSKEY_USAGE_COLOR" android:summary="@string/prefs_summary_usage_color" />
