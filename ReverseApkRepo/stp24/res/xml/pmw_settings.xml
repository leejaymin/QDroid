        <CheckBoxPreference android:title="@string/prefs_screen_main_popup" android:key="@string/PREFSKEY_MAIN_POPUP" android:defaultValue="false" android:summaryOn="@string/prefs_screen_main_popup_on" android:summaryOff="@string/prefs_screen_main_popup_off" />
        <CheckBoxPreference android:title="@string/prefs_main_auto_close" android:key="@string/PREFSKEY_AUTO_CLOSE_MAIN" android:defaultValue="false" android:summaryOn="@string/prefs_main_auto_close_on" android:summaryOff="@string/prefs_main_auto_close_off" />
