        </PreferenceScreen>
    </PreferenceScreen>
    <PreferenceScreen android:title="@string/prefs_screen_notifications" android:summary="@string/prefs_screen_notifications_summary">
        <Preference android:title="@string/prefs_title_notif_type" android:key="@string/PREFSKEY_NOTIF_PACKAGE" android:summary="@string/prefs_summary_notif_type" />
        <ListPreference android:entries="@array/settings_notif_refresh_rates" android:title="@string/prefs_title_notif_refresh_rate" android:key="@string/PREFSKEY_NOTIFICATION_REFRESH_RATE" android:summary="@string/prefs_summary_notif_refresh_rate" android:defaultValue="10" android:entryValues="@array/settings_notif_refresh_rate_values" />
        <ListPreference android:entries="@array/settings_widget_refresh_rates" android:title="@string/prefs_title_widget_refresh_rate" android:key="@string/PREFSKEY_WIDGET_REFRESH_RATE" android:summary="@string/prefs_summary_widget_refresh_rate" android:defaultValue="60" android:entryValues="@array/settings_widget_refresh_rate_values" />
