            <CheckBoxPreference android:title="@string/prefs_title_record_screen" android:key="@string/PREFSKEY_RECORD_SCREEN" android:defaultValue="true" android:summaryOn="@string/prefs_record_on" android:summaryOff="@string/prefs_record_off" />
            <CheckBoxPreference android:title="@string/prefs_title_record_wifi" android:key="@string/PREFSKEY_RECORD_WIFI" android:defaultValue="false" android:summaryOn="@string/prefs_record_on" android:summaryOff="@string/prefs_record_off" />
            <CheckBoxPreference android:title="@string/prefs_title_record_bt" android:key="@string/PREFSKEY_RECORD_BT" android:defaultValue="false" android:summaryOn="@string/prefs_record_on" android:summaryOff="@string/prefs_record_off" />
