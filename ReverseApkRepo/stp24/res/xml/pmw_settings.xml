            <CheckBoxPreference android:title="@string/prefs_title_exclude_system" android:key="@string/PREFSKEY_EXCLUDE_SYSTEM" android:defaultValue="true" android:summaryOn="@string/prefs_exclude_system_on" android:summaryOff="@string/prefs_exclude_system_off" />
