<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff444444" android:endColor="#ff444444" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff666666" android:endColor="#ff666666" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
</selector>
