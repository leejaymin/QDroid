<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#ccffffff" />
            <corners android:radius="4.0dip" />
            <gradient android:startColor="#ff222222" android:endColor="#ff333333" android:angle="90.0" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#ccffffff" />
            <corners android:radius="4.0dip" />
            <gradient android:startColor="#ffefc83f" android:endColor="#ffd49a24" android:angle="90.0" />
        </shape>
    </item>
</selector>
