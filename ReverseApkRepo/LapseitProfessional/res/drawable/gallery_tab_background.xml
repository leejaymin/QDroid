<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff1b6069" android:endColor="#ff2d95a8" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff2a2a2a" android:endColor="#ff555555" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff135d67" android:endColor="#ff10a9c7" android:angle="90.0" android:type="linear" />
        </shape>
    </item>
</selector>
