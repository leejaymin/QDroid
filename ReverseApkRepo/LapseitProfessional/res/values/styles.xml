<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="previewbar">
        <item name="android:maxHeight">5.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progress_horizontal</item>
        <item name="android:minHeight">5.0dip</item>
    </style>
    <style name="Theme.LapseIt.NoTitle.FullScreen" parent="@android:style/Theme">
        <item name="android:windowBackground">@drawable/background_shelf</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="Theme.LapseIt.NoTitle.FullScreen.NoBackground" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/mainbackground</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="Theme.LapseIt.Title.Fullscreen" parent="@android:style/Theme">
        <item name="android:windowBackground">@drawable/background_shelf</item>
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="Theme.LapseIt.NoTitle" parent="@android:style/Theme">
        <item name="android:windowBackground">@drawable/background_shelf</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">false</item>
    </style>
    <style name="Theme.LapseIt.Title" parent="@android:style/Theme">
        <item name="android:windowBackground">@drawable/background_shelf</item>
        <item name="android:windowNoTitle">false</item>
        <item name="android:windowFullscreen">false</item>
    </style>
</resources>
