<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:textColor="#ffffffff" android:id="@id/Title" android:background="#ff888888" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/app_name" />
    <TableLayout android:id="@id/TableLayout01" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TableRow android:id="@id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <CheckedTextView android:id="@id/CheckedTextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/avrserviceCheckBox" />
            <CheckBox android:id="@id/ServiceCheckBox" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" android:hapticFeedbackEnabled="true" />
        </TableRow>
        <TextView android:textColor="#ffffffff" android:id="@id/SubTitle2" android:background="#ff333333" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/subTitle2Text" />
        <RadioGroup android:id="@id/RadioGroup02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="18.0px" android:hapticFeedbackEnabled="true">
            <TableLayout android:id="@id/TableLayout02" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TableRow android:id="@id/TableRow05" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <CheckedTextView android:id="@id/CheckedTextViewNotificationVibrate" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/CheckedTextNotificationVibrateString" />
                    <RadioButton android:id="@id/RadioButtonNotificationsVibrate" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" />
                </TableRow>
                <TableRow android:id="@id/TableRow04" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <CheckedTextView android:id="@id/CheckedTextSilentNotification" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/CheckedTextSilentNotificationString" />
                    <RadioButton android:id="@id/RadioButtonNotificationSilent" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="false" />
                </TableRow>
                <TableRow android:id="@id/TableRow06" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <CheckedTextView android:id="@id/CheckedTextViewNoisyVibrate" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/CheckedTextNotificationNoisyVibrate" />
                    <RadioButton android:id="@id/RadioButtonNotificationNoisy" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="false" />
                </TableRow>
            </TableLayout>
        </RadioGroup>
        <TextView android:textColor="#ffffffff" android:id="@id/SubTitle1" android:background="#ff333333" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/subTitle1Text" />
        <RadioGroup android:id="@id/RadioGroup01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="18.0px" android:hapticFeedbackEnabled="true">
            <TableLayout android:id="@id/TableLayout03" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TableRow android:id="@id/TableRow02" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <CheckedTextView android:id="@id/CheckedTextView02" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/RadioTextMagHolster" />
                    <RadioButton android:id="@id/RadioButtonMagHolster" android:visibility="invisible" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="true" />
                </TableRow>
                <TableRow android:id="@id/TableRow03" android:layout_width="wrap_content" android:layout_height="wrap_content">
                    <CheckedTextView android:id="@id/CheckedTextView03" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/RadioTextCarOrDeskDock" />
                    <RadioButton android:id="@id/RadioButtonCarOrDeskDock" android:visibility="invisible" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:checked="false" />
                </TableRow>
            </TableLayout>
        </RadioGroup>
    </TableLayout>
</LinearLayout>
